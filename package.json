{
  "name": "bart",
  "version": "1.0.0",
  "private": true,
  "description": "Tool to catch bart on time.",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "concurrently -k \"tsc -w -p tsconfig.json\" \"node server.js\"",
    "start": "node server.js",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.30",
    "concurrently": "^9.1.2",
    "typescript": "^5.4.5"
  }
}
